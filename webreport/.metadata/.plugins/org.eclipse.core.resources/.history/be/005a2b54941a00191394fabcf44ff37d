// A $( document ).ready() block.
$( document ).ready(function() {
    console.log( "ready!" );
    $.ajax({
    	type: 'Get',
        url: 'https://olympus--fis.cs60.my.salesforce.com/services/data/v42.0/query/?q=SELECT+Id,BillingStreet,BillingCity,BillingState+from+Account ',
        dataType: "json",
        success: function(data) {
        	
        	
        },
        error:	function(data) {
        	console.log(data);
        },
    	
    });
    
    
    
});