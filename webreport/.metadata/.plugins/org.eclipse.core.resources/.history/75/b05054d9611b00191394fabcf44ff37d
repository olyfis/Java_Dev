// A $( document ).ready() block.
$( document ).ready(function() {
    console.log( "ready!" );
    $.ajax({
    	type: 'Get',
        url: 'http://localhost:8181/webreport/getchart',
        dataType: "json",
        success: function(data) {
        	console.log(data);
        	var pending = [];
        	var releases = [];
        	
        	for (var i in data) {
        		pending.push("pending" + data[i].id)
        	}
        	
        	
        },
        error:	function(data) {
        	console.log(data);
        },
    	
    });
    
    
    
});