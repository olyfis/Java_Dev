// A $( document ).ready() block.
$( document ).ready(function() {
    console.log( "ready!" );
    $.ajax({
    	type: 'Get',
        url: 'https://api.github.com/users/mralexgray/repos',
        dataType: "json",
        success: function(data) {
        	
        	console.log(data);
        },
        error:	function(data) {
        	console.log(data);
        },
    	
    });
    
    
    
});