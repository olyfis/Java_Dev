package com.olympus.rest;

/* http://localhost:8181/dashboard/fis/getdate */

import javax.ws.*;
import javax.ws.rs.*;
import javax.ws.rs.core.MediaType;
import com.olympus.rest.*;

import java.io.File;
import java.io.IOException;




@Path("getdate")
public class FileDate {

	@GET
	@Produces(MediaType.TEXT_HTML)
	 

	//public String returnLastModifyDate(@PathParam("filename") throws IOException {
	
	public String returnLastModifyDate() throws IOException {
		String date1 = null;
	    
	    File file = new File("D:/Kettle/XML/dashboard.html");
	    
	    
	    date1 = com.olympus.rest.GetFileAttrs.getFileDate(file);
	    
		return "" + date1 +  "";
	}

}
